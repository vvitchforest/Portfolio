    "autoprefixer": "^9.1.5",
    "bootstrap": "^4.1.3",
    "chokidar-cli": "^1.2.1",
    "node-sass-chokidar": "^1.3.3",
    "postcss-cli": "^6.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
